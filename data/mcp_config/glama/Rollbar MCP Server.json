{
  "mcpServers": {
    "rollbar-mcp": {
      "command": "npx",
      "args": [
        "-y",
        "@hiyorineko/mcp-rollbar-server"
      ],
      "env": {
        "ROLLBAR_PROJECT_TOKEN": "YOUR_PROJECT_ACCESS_TOKEN",
        "ROLLBAR_ACCOUNT_TOKEN": "YOUR_ACCOUNT_ACCESS_TOKEN",
        "ROLLBAR_PROJECT_ID": "YOUR_PROJECT_ID",
        "ROLLBAR_PROJECT_NAME": "YOUR_PROJECT_NAME"
      }
    }
  }
}