    {
        "name": "meilisearch_create-chat-completion",
        "description": "Create a conversational chat completion using Meilisearch's chat feature",
        "parameters": {
            "type": "object",
            "properties": {
                "workspace_uid": {
                    "type": "string",
                    "description": "Unique identifier of the chat workspace"
                },
                "messages": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "role": {
                                "type": "string",
                                "enum": [
                                    "user",
                                    "assistant",
                                    "system"
                                ]
                            },
                            "content": {
                                "type": "string"
                            }
                        },
                        "required": [
                            "role",
                            "content"
                        ]
                    },
                    "description": "List of message objects comprising the chat history"
                },
                "model": {
                    "type": "string",
                    "default": "gpt-3.5-turbo",
                    "description": "The model to use for completion"
                },
                "stream": {
                    "type": "boolean",
                    "default": true,
                    "description": "Whether to stream the response (currently must be true)"
                }
            },
            "required": [
                "workspace_uid",
                "messages"
            ],
            "additionalProperties": false
        }
    },
