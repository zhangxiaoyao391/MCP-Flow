                },
                "h": {
                    "type": "number",
                    "description": "Object's maximum absolute magnitude (mag). Values: 16-30"
                },
                "occ": {
                    "type": "number",
                    "description": "Object's maximum orbit condition code. Values: 0-8"
                },
                "des": {
                    "type": "string",
                    "description": "Object designation (e.g., '2000 SG344' or '433')"
                },
                "spk": {
                    "type": "string",
