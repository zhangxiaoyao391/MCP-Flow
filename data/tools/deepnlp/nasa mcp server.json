    {
        "name": "jpl_nhats",
        "description": "Human-accessible NEOs (Near-Earth Objects) data",
        "parameters": {
            "type": "object",
            "properties": {
                "dv": {
                    "type": "number",
                    "description": "Minimum total delta-V (km/s). Values: 4-12, default: 12"
                },
                "dur": {
                    "type": "number",
                    "description": "Minimum total mission duration (days). Values: 60-450, default: 450"
                },
