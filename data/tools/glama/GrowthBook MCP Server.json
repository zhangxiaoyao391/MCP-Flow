    {
        "name": "growthbook_create_sdk_connection",
        "description": "Create an SDK connection for a user. Returns an SDK clientKey that can be used to fetch features and experiments.",
        "parameters": {
            "type": "object",
            "properties": {
                "name": {
                    "type": "string",
                    "description": "Name of the SDK connection in GrowthBook. Should reflect the current project."
                },
                "language": {
                    "type": "string",
                    "enum": [
                        "nocode-webflow",
                        "nocode-wordpress",
                        "nocode-shopify",
                        "nocode-other",
                        "javascript",
                        "nodejs",
                        "react",
                        "php",
                        "ruby",
                        "python",
                        "go",
                        "java",
                        "csharp",
                        "android",
                        "ios",
                        "flutter",
                        "elixir",
                        "edge-cloudflare",
                        "edge-fastly",
                        "edge-lambda",
                        "edge-other",
                        "other"
                    ],
                    "description": "The language of the SDK. Either 'javascript' or 'typescript'."
                },
                "environment": {
                    "type": "string",
                    "description": "The environment associated with the SDK connection."
                }
            },
            "required": [
                "name",
                "language"
            ],
            "additionalProperties": false,
            "$schema": "http://json-schema.org/draft-07/schema#"
        }
    },
    {
        "name": "growthbook_create_feature_flag",
        "description": "Creates a new feature flag in GrowthBook and modifies the codebase when relevant.",
        "parameters": {
            "type": "object",
            "properties": {
                "id": {
                    "type": "string",
                    "pattern": "^[a-zA-Z0-9_-]+$",
                    "description": "A unique key name for the feature"
                },
                "description": {
                    "type": "string",
                    "default": "",
                    "description": "A brief description of the feature flag"
                },
                "valueType": {
                    "type": "string",
                    "enum": [
                        "string",
                        "number",
                        "boolean",
                        "json"
                    ],
                    "description": "The value type the feature flag will return"
                },
                "defaultValue": {
                    "type": "string",
                    "description": "The default value of the feature flag"
                },
                "fileExtension": {
                    "type": "string",
                    "enum": [
                        ".tsx",
                        ".jsx",
                        ".ts",
                        ".js",
                        ".vue",
                        ".py",
                        ".go",
                        ".php",
                        ".rb",
                        ".java",
                        ".cs",
                        ".swift",
                        ".ex",
                        ".exs",
                        ".kt",
                        ".kts",
                        ".ktm",
                        ".dart"
                    ],
                    "description": "The extension of the current file. If it's unclear, ask the user."
                }
            },
            "required": [
                "id",
                "valueType",
                "defaultValue",
                "fileExtension"
            ],
            "additionalProperties": false,
            "$schema": "http://json-schema.org/draft-07/schema#"
        }
    },
