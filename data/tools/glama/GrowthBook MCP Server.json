    {
        "name": "growthbook_create_experiment",
        "description": "IMPORTANT: Call get_defaults before creating an experiment, and use its output to guide the arguments. Creates a new feature flag and experiment (A/B test).",
        "parameters": {
            "type": "object",
            "properties": {
                "name": {
                    "type": "string",
                    "description": "Experiment name. Base name off the examples from get_defaults. If none are available, use a short, descriptive name that captures the essence of the experiment."
                },
                "description": {
                    "type": "string",
                    "description": "Experiment description."
                },
                "hypothesis": {
                    "type": "string",
                    "description": "Experiment hypothesis. Base hypothesis off the examples from get_defaults. If none are available, use a falsifiable statement about what will happen if the experiment succeeds or fails."
                },
                "value": {
                    "type": "string",
                    "description": "The default value of the experiment."
                },
                "variations": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "name": {
                                "type": "string",
                                "description": "Variation name. Base name off the examples from get_defaults. If none are available, use a short, descriptive name that captures the essence of the variation."
                            },
                            "value": {
                                "anyOf": [
                                    {
                                        "type": "string"
                                    },
                                    {
                                        "type": "number"
                                    },
                                    {
                                        "type": "boolean"
                                    },
                                    {
                                        "type": "object",
                                        "additionalProperties": {}
                                    }
                                ],
                                "description": "The value of the control and each of the variations. The value should be a string, number, boolean, or object. If it's an object, it should be a valid JSON object."
                            }
                        },
                        "required": [
                            "name",
                            "value"
                        ],
                        "additionalProperties": false
                    },
                    "description": "Experiment variations. The key should be the variation name and the value should be the variation value. Look to variations included in preview experiments for guidance on generation. The default or control variation should always be first."
                },
                "fileExtension": {
                    "type": "string",
                    "enum": [
                        ".tsx",
                        ".jsx",
                        ".ts",
                        ".js",
                        ".vue",
                        ".py",
                        ".go",
                        ".php",
                        ".rb",
                        ".java",
                        ".cs",
                        ".swift",
                        ".ex",
                        ".exs",
                        ".kt",
                        ".kts",
                        ".ktm",
                        ".dart"
                    ],
                    "description": "The extension of the current file. If it's unclear, ask the user."
                },
                "confirmedDefaultsReviewed": {
                    "type": "boolean",
                    "description": "Set to true to confirm you have called get_defaults and reviewed the output to guide these parameters."
                }
            },
            "required": [
                "name",
                "value",
                "variations",
                "fileExtension",
                "confirmedDefaultsReviewed"
            ],
            "additionalProperties": false,
            "$schema": "http://json-schema.org/draft-07/schema#"
        }
    },
