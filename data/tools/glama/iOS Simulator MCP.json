[
    {
        "name": "ios-simulator_get_booted_sim_id",
        "description": "Get the ID of the currently booted iOS simulator",
        "parameters": {
            "type": "object",
            "properties": {}
        }
    },
    {
        "name": "ios-simulator_ui_describe_all",
        "description": "Describes accessibility information for the entire screen in the iOS Simulator",
        "parameters": {
            "type": "object",
            "properties": {
                "udid": {
                    "type": "string",
                    "pattern": "^[0-9A-Fa-f]{8}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{12}$",
                    "description": "Udid of target, can also be set with the IDB_UDID env var"
                }
            },
            "additionalProperties": false,
            "$schema": "http://json-schema.org/draft-07/schema#"
        }
    },
    {
        "name": "ios-simulator_ui_tap",
        "description": "Tap on the screen in the iOS Simulator",
        "parameters": {
            "type": "object",
            "properties": {
                "duration": {
                    "type": "string",
                    "pattern": "^\\d+(\\.\\d+)?$",
                    "description": "Press duration"
                },
                "udid": {
                    "type": "string",
                    "pattern": "^[0-9A-Fa-f]{8}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{12}$",
                    "description": "Udid of target, can also be set with the IDB_UDID env var"
                },
                "x": {
                    "type": "number",
                    "description": "The x-coordinate"
                },
                "y": {
                    "type": "number",
                    "description": "The x-coordinate"
                }
            },
            "required": [
                "x",
                "y"
            ],
            "additionalProperties": false,
            "$schema": "http://json-schema.org/draft-07/schema#"
        }
    },
    {
        "name": "ios-simulator_ui_type",
        "description": "Input text into the iOS Simulator",
        "parameters": {
            "type": "object",
            "properties": {
                "udid": {
                    "type": "string",
                    "pattern": "^[0-9A-Fa-f]{8}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{12}$",
                    "description": "Udid of target, can also be set with the IDB_UDID env var"
                },
                "text": {
                    "type": "string",
                    "maxLength": 500,
                    "pattern": "^[\\x20-\\x7E]+$",
                    "description": "Text to input"
                }
            },
            "required": [
                "text"
            ],
            "additionalProperties": false,
            "$schema": "http://json-schema.org/draft-07/schema#"
        }
    },
    {
        "name": "ios-simulator_ui_swipe",
        "description": "Swipe on the screen in the iOS Simulator",
        "parameters": {
            "type": "object",
            "properties": {
                "udid": {
                    "type": "string",
                    "pattern": "^[0-9A-Fa-f]{8}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{12}$",
                    "description": "Udid of target, can also be set with the IDB_UDID env var"
                },
                "x_start": {
                    "type": "number",
                    "description": "The starting x-coordinate"
                },
                "y_start": {
                    "type": "number",
                    "description": "The starting y-coordinate"
                },
                "x_end": {
                    "type": "number",
                    "description": "The ending x-coordinate"
                },
                "y_end": {
                    "type": "number",
                    "description": "The ending y-coordinate"
                },
                "delta": {
                    "type": "number",
                    "description": "The size of each step in the swipe (default is 1)",
                    "default": 1
                }
            },
            "required": [
                "x_start",
                "y_start",
                "x_end",
                "y_end"
            ],
            "additionalProperties": false,
            "$schema": "http://json-schema.org/draft-07/schema#"
        }
    },
    {
        "name": "ios-simulator_ui_describe_point",
        "description": "Returns the accessibility element at given co-ordinates on the iOS Simulator's screen",
        "parameters": {
            "type": "object",
            "properties": {
                "udid": {
                    "type": "string",
                    "pattern": "^[0-9A-Fa-f]{8}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{12}$",
                    "description": "Udid of target, can also be set with the IDB_UDID env var"
                },
                "x": {
                    "type": "number",
                    "description": "The x-coordinate"
                },
                "y": {
                    "type": "number",
                    "description": "The y-coordinate"
                }
            },
            "required": [
                "x",
                "y"
            ],
            "additionalProperties": false,
            "$schema": "http://json-schema.org/draft-07/schema#"
        }
    },
    {
        "name": "ios-simulator_ui_view",
        "description": "Get the image content of a compressed screenshot of the current simulator view",
        "parameters": {
            "type": "object",
            "properties": {
                "udid": {
                    "type": "string",
                    "pattern": "^[0-9A-Fa-f]{8}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{12}$",
                    "description": "Udid of target, can also be set with the IDB_UDID env var"
                }
            },
            "additionalProperties": false,
            "$schema": "http://json-schema.org/draft-07/schema#"
        }
    }
]