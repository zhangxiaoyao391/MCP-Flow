[
    {
        "name": "jina-ai-mcp-server_read_webpage",
        "description": "Extract content from a webpage in a format optimized for LLMs",
        "parameters": {
            "type": "object",
            "properties": {
                "url": {
                    "type": "string"
                },
                "format": {
                    "type": "string",
                    "enum": [
                        "Default",
                        "Markdown",
                        "HTML",
                        "Text",
                        "Screenshot",
                        "Pageshot"
                    ]
                },
                "with_links": {
                    "type": "boolean"
                },
                "with_images": {
                    "type": "boolean"
                },
                "with_generated_alt": {
                    "type": "boolean"
                },
                "no_cache": {
                    "type": "boolean"
                }
            },
            "required": [
                "url"
            ],
            "additionalProperties": false,
            "$schema": "http://json-schema.org/draft-07/schema#"
        }
    },
    {
        "name": "jina-ai-mcp-server_search_web",
        "description": "Search the web using Jina AI's search API",
        "parameters": {
            "type": "object",
            "properties": {
                "query": {
                    "type": "string"
                },
                "count": {
                    "type": "number",
                    "default": 5
                },
                "retain_images": {
                    "type": "string",
                    "enum": [
                        "none",
                        "all"
                    ],
                    "default": "none"
                },
                "with_generated_alt": {
                    "type": "boolean",
                    "default": true
                },
                "return_format": {
                    "type": "string",
                    "enum": [
                        "markdown",
                        "text",
                        "html"
                    ],
                    "default": "markdown"
                }
            },
            "required": [
                "query"
            ],
            "additionalProperties": false,
            "$schema": "http://json-schema.org/draft-07/schema#"
        }
    },
    {
        "name": "jina-ai-mcp-server_fact_check",
        "description": "Fact-check a statement using Jina AI's grounding engine",
        "parameters": {
            "type": "object",
            "properties": {
                "statement": {
                    "type": "string"
                },
                "deepdive": {
                    "type": "boolean",
                    "default": false
                }
            },
            "required": [
                "statement"
            ],
            "additionalProperties": false,
            "$schema": "http://json-schema.org/draft-07/schema#"
        }
    }
]