                    "description": "Number of adult guests (13 years and over) Default: 1",
                    "title": "Adults"
                },
                "children": {
                    "anyOf": [
                        {
                            "type": "integer"
                        },
                        {
                            "type": "number"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "Number of children (2-12 years) Default: 0",
                    "title": "Children"
                },
                "infants": {
                    "anyOf": [
                        {
                            "type": "integer"
                        },
                        {
                            "type": "number"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "Number of infants (under 2 years) Default: 0",
                    "title": "Infants"
                },
                "pets": {
                    "anyOf": [
                        {
                            "type": "integer"
                        },
                        {
                            "type": "number"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "Number of pets Default: 0",
                    "title": "Pets"
                },
                "page": {
                    "anyOf": [
                        {
                            "type": "integer"
                        },
                        {
                            "type": "number"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "Page of results returned Allowed values: 1-8 Default: 1",
                    "title": "Page"
                },
                "currency": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "Price currency, default: USD",
                    "title": "Currency"
                },
                "min_beds": {
                    "anyOf": [
                        {
                            "type": "integer"
                        },
                        {
                            "type": "number"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "Minimum number of beds",
                    "title": "Min Beds"
                },
                "min_bedrooms": {
                    "anyOf": [
                        {
                            "type": "integer"
                        },
                        {
                            "type": "number"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "Minimum number of bedrooms",
                    "title": "Min Bedrooms"
                }
            },
            "required": [
                "location",
                "checkin",
                "checkout",
                "adults"
            ],
            "type": "object"
        }
    },
