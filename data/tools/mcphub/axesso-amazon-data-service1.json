    {
        "name": "deals",
        "description": "Lookup current deals available on Amazon (Today's Deals / Top Deals, Best Deals, and Lightning Deals) for different marketplaces using various filter options like department, price and discount.",
        "parameters": {
            "properties": {
                "domainCode": {
                    "description": "all available marketplaces like com, de, co.uk, fr, it and other",
                    "title": "Domaincode",
                    "type": "string"
                },
                "page": {
                    "anyOf": [
                        {
                            "type": "integer"
                        },
                        {
                            "type": "number"
                        }
                    ],
                    "description": "Use this parameter to request deals page by page. Each page includes up to 30 deals Default: 1",
                    "title": "Page"
                },
                "departments": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "Returns deals within this provided department. Department id can be either fetched from amazon page via browser or using our API endpoint 'dealFilter'",
                    "title": "Departments"
                },
                "discountRange": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "Returns deals within a this discount range. Find an overview of the possible values in our endpoint 'dealFilter' Valid values are 1, 2, 3, 4,5",
                    "title": "Discountrange"
                },
                "minProductStarRating": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "Returns deals within a this review range. Find an overview of the possible values in our endpoint 'dealFilter' Valid values are 1, 2, 3, 4",
                    "title": "Minproductstarrating"
                },
                "priceRange": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "Returns deals within a this price range. Find an overview of the possible values in our endpoint 'dealFilter' Valid values are 1, 2, 3, 4, 5",
                    "title": "Pricerange"
                },
                "primeEarly": {
                    "anyOf": [
                        {
                            "type": "boolean"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "Returns prime early deals only",
                    "title": "Primeearly"
                },
                "primeExclusive": {
                    "anyOf": [
                        {
                            "type": "boolean"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "Returns prime exclusive deals only",
                    "title": "Primeexclusive"
                }
            },
            "required": [
                "domainCode",
                "page"
            ],
            "type": "object"
        }
