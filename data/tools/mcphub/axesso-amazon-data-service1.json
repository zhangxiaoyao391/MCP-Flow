    {
        "name": "search_products",
        "description": "Search for products on Amazon by keyword and return ASINs. Supports filtering by category, sort order, and more.",
        "parameters": {
            "properties": {
                "domainCode": {
                    "description": "Amazon domain suffix (e.g., com, de, fr, co.uk).",
                    "title": "Domaincode",
                    "type": "string"
                },
                "keyword": {
                    "description": "keyword used for the search",
                    "title": "Keyword",
                    "type": "string"
                },
                "page": {
                    "description": "Parameter for Pagination",
                    "title": "Page",
                    "type": "string"
                },
                "excludeSponsored": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "Set true to exclude sponsored products",
                    "title": "Excludesponsored"
                },
                "sortBy": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "sorting for the search results",
                    "title": "Sortby"
                },
                "withCache": {
                    "anyOf": [
                        {
                            "type": "boolean"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "If true, proxy for same searchterm will be cached and used for next page. Might increase the response quality.",
                    "title": "Withcache"
                },
                "category": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "Pass the amazon category which should used for the search. Valid category list can found on the amazon website on the search selection box. Important: If the passed category is not a valid amazon category, the response will be empty",
                    "title": "Category"
                },
                "browseNode": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "Define the current browsing node (category or sub-category). E.g. pass 10158976011 to get Amazon Resale",
                    "title": "Browsenode"
                },
                "nodeHierarchy": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "Further category nodes hierarchy. Can be a single category number or a comma separated list, e.g. 3375251,10158976011,706814011,11051400011",
                    "title": "Nodehierarchy"
                }
            },
            "required": [
                "domainCode",
                "keyword",
                "page"
            ],
            "type": "object"
        }
    },
