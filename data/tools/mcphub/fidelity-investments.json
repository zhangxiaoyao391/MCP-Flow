        "name": "news_list_by_symbol",
        "description": "Get news headlines by symbol",
        "parameters": {
            "properties": {
                "symbol": {
                    "description": "The value of symbol field returned in .../v4/auto-complete endpoint",
                    "title": "Symbol",
                    "type": "string"
                },
                "endDate": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "List news before this point of time. The date format must be yyyy-MM-dd'T'HH:mm:ss Ex : 2024-01-25T12:12:12",
                    "title": "Enddate"
                },
                "count": {
                    "anyOf": [
                        {
                            "type": "integer"
                        },
                        {
                            "type": "number"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "Limit number of items per response. Maximum is 50 Default: 21",
                    "title": "Count"
                }
            },
            "required": [
                "symbol"
            ],
            "type": "object"
        }
    },
    {
        "name": "v2_get_details",
