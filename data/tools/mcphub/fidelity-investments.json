    {
        "name": "quotes_get_chart",
        "description": "Get information to draw chart",
        "parameters": {
            "properties": {
                "symbol": {
                    "description": "Separated by comma for multiple symbols, support up to 3 symbols at a time",
                    "title": "Symbol",
                    "type": "string"
                },
                "startDate": {
                    "description": "Date format must be strictly follow yyyy/MM/dd-HH:mm:ss",
                    "title": "Startdate",
                    "type": "string"
                },
                "endDate": {
                    "description": "Date format must be strictly follow yyyy/MM/dd-HH:mm:ss",
                    "title": "Enddate",
                    "type": "string"
                },
                "intraday": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "Y or N",
                    "title": "Intraday"
                },
                "granularity": {
                    "anyOf": [
                        {
                            "type": "integer"
                        },
                        {
                            "type": "number"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "From 1 to 6, use with intraday to specify day or month Default: 1",
                    "title": "Granularity"
                }
            },
            "required": [
                "symbol",
                "startDate",
                "endDate"
            ],
            "type": "object"
        }
    },
