[
    {
        "name": "search_google_play",
        "description": "Search Google Play Apps Store using SerpApi",
        "parameters": {
            "properties": {
                "q": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "Search query. Cannot be used with apps_category or store_device.",
                    "title": "Q"
                },
                "apps_category": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "App store category. Cannot be used with q or store_device.",
                    "title": "Apps Category"
                },
                "store_device": {
                    "anyOf": [
                        {
                            "enum": [
                                "phone",
                                "tablet",
                                "tv",
                                "chromebook",
                                "watch",
                                "car"
                            ],
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "Device type. Cannot be used with q or apps_category.",
                    "title": "Store Device"
                },
                "age": {
                    "anyOf": [
                        {
                            "enum": [
                                "AGE_RANGE1",
                                "AGE_RANGE2",
                                "AGE_RANGE3"
                            ],
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "Age range. Only works with apps_category=FAMILY.",
                    "title": "Age"
                },
                "next_page_token": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "Next page token for pagination.",
                    "title": "Next Page Token"
                },
                "section_page_token": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "Section page token for pagination.",
                    "title": "Section Page Token"
                },
                "chart": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "Chart type (e.g., 'topselling_free').",
                    "title": "Chart"
                },
                "see_more_token": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "'See more' token for pagination.",
                    "title": "See More Token"
                },
                "no_cache": {
                    "anyOf": [
                        {
                            "type": "boolean"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "Disallow cached results.",
                    "title": "No Cache"
                },
                "aasync": {
                    "anyOf": [
                        {
                            "type": "boolean"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "Use async mode.",
                    "title": "Aasync"
                },
                "zero_trace": {
                    "anyOf": [
                        {
                            "type": "boolean"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "Enterprise only: Enable ZeroTrace mode.",
                    "title": "Zero Trace"
                },
                "output": {
                    "anyOf": [
                        {
                            "enum": [
                                "json",
                                "html"
                            ],
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "Output format: 'json' or 'html'.",
                    "title": "Output"
                }
            },
            "title": "search_google_playArguments",
            "type": "object"
        }
    }
]