[
    {
        "name": "get_meta_data",
        "description": "Get init meta data",
        "parameters": {
            "properties": {
                "locale_info": {
                    "description": "The language code",
                    "title": "Locale Info",
                    "type": "string"
                },
                "lang_ID": {
                    "anyOf": [
                        {
                            "type": "integer"
                        },
                        {
                            "type": "number"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "The value of all_langs/lang_ID returned in .../get-meta-data endpoint Default: 1",
                    "title": "Lang Id"
                },
                "time_utc_offset": {
                    "anyOf": [
                        {
                            "type": "integer"
                        },
                        {
                            "type": "number"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "UTC value in seconds, for example : utc+8 -> 8 * 60 * 60 = 28800 Default: 28800",
                    "title": "Time Utc Offset"
                }
            },
            "required": [
                "locale_info"
            ],
            "type": "object"
        }
    },
    {
        "name": "get_ico_calendar",
        "description": "Get ICO calendar",
        "parameters": {
            "properties": {
                "tabname": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "One of the following : upcoming|completed|ongoing",
                    "title": "Tabname"
                },
                "lang_ID": {
                    "anyOf": [
                        {
                            "type": "integer"
                        },
                        {
                            "type": "number"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "The value of all_langs/lang_ID returned in .../get-meta-data endpoint Default: 1",
                    "title": "Lang Id"
                },
                "time_utc_offset": {
                    "anyOf": [
                        {
                            "type": "integer"
                        },
                        {
                            "type": "number"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "UTC value in seconds, for example : utc+8 -> 8 * 60 * 60 = 28800 Default: 28800",
                    "title": "Time Utc Offset"
                },
                "category": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "Check for suitable value of icoData/categories returned right in this endpoint. Separated by comma for multiple options. For example : _ico_cat_gaming,_ico_cat_ecomm,_ico_cat_finance,_ico_cat_healthcare",
                    "title": "Category"
                },
                "sort": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "One of the following is allowed related_days | name | funds_raised | completed",
                    "title": "Sort"
                }
            },
            "type": "object"
        }
    },
