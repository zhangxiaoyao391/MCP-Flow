[
    {
        "name": "get_market_movers",
        "description": "\n    Get market movers: top gainers, losers, or most active stocks.\n    Supports different market sessions for most-active category.\n\n    Args:\n        category: Type of movers to fetch\n        count: Number of stocks to return (max 100)\n        market_session: Market session (only applies to most-active category)\n\n    Returns:\n        Dictionary with market mover data\n    ",
        "parameters": {
            "properties": {
                "category": {
                    "default": "most-active",
                    "enum": [
                        "gainers",
                        "losers",
                        "most-active"
                    ],
                    "title": "Category",
                    "type": "string"
                },
                "count": {
                    "default": 25,
                    "title": "Count",
                    "type": "integer"
                },
                "market_session": {
                    "default": "regular",
                    "enum": [
                        "regular",
                        "pre-market",
                        "after-hours"
                    ],
                    "title": "Market Session",
                    "type": "string"
                }
            },
            "title": "get_market_moversArguments",
            "type": "object"
        }
    },
