[
    {
        "name": "ask-openai",
        "description": "Ask my assistant models a direct question",
        "parameters": {
            "type": "object",
            "properties": {
                "query": {
                    "type": "string",
                    "description": "Ask assistant"
                },
                "model": {
                    "type": "string",
                    "default": "gpt-4",
                    "enum": [
                        "gpt-4",
                        "gpt-3.5-turbo"
                    ]
                },
                "temperature": {
                    "type": "number",
                    "default": 0.7,
                    "minimum": 0,
                    "maximum": 2
                },
                "max_tokens": {
                    "type": "integer",
                    "default": 500,
                    "minimum": 1,
                    "maximum": 4000
                }
            },
            "required": [
                "query"
            ]
        }
    }
]