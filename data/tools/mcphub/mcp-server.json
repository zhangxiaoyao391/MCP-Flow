                "desiredBrowserVersion": {
                    "type": "string",
                    "description": "Browser version for desktop (e.g. '133.2', 'latest'). If the user says 'latest', 'newest', or similar, normalize it to 'latest'. Likewise, convert terms like 'earliest' or 'oldest' to 'oldest'."
                },
                "desiredDevice": {
                    "type": "string",
                    "description": "Device name for mobile"
                }
