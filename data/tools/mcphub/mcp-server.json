    {
        "name": "runAppTestsOnBrowserStack",
        "description": "Run AppAutomate tests on BrowserStack by uploading app and test suite. If running from Android Studio or Xcode, the tool will help export app and test files automatically. For other environments, you'll need to provide the paths to your pre-built app and test files.",
        "parameters": {
            "type": "object",
            "properties": {
                "appPath": {
                    "type": "string",
                    "description": "Path to your application file:\nIf in development IDE directory:\nâ€¢ For Android: 'gradle assembleDebug'\nâ€¢ For iOS:\n  xcodebuild clean -scheme YOUR_SCHEME && \\\n  xcodebuild archive -scheme YOUR_SCHEME -configuration Release -archivePath build/app.xcarchive && \\\n  xcodebuild -exportArchive -archivePath build/app.xcarchive -exportPath build/ipa -exportOptionsPlist exportOptions.plist\n\nIf in other directory, provide existing app path"
                },
                "testSuitePath": {
                    "type": "string",
                    "description": "Path to your test suite file:\nIf in development IDE directory:\nâ€¢ For Android: 'gradle assembleAndroidTest'\nâ€¢ For iOS:\n  xcodebuild test-without-building -scheme YOUR_SCHEME -destination 'generic/platform=iOS' && \\\n  cd ~/Library/Developer/Xcode/DerivedData/*/Build/Products/Debug-iphonesimulator/ && \\\n  zip -r Tests.zip *.xctestrun *-Runner.app\n\nIf in other directory, provide existing test file path"
                },
                "devices": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    },
                    "description": "List of devices to run the test on, e.g., ['Samsung Galaxy S20-10.0', 'iPhone 12 Pro-16.0']."
                },
