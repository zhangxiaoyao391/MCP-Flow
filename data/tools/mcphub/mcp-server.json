                "desiredPlatforms": {
                    "type": "array",
                    "items": {
                        "type": "string",
                        "enum": [
                            "windows",
                            "macos",
                            "android",
                            "ios"
                        ]
                    },
                    "description": "The platforms the user wants to test on. Always ask this to the user, do not try to infer this."
                },
                "enablePercy": {
                    "type": "boolean",
                    "default": false,
                    "description": "Set to true if the user wants to enable Percy for visual testing. Defaults to false."
