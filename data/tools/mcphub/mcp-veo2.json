[
    {
        "name": "generateVideoFromText",
        "description": "Generate a video from a text prompt",
        "parameters": {
            "type": "object",
            "properties": {
                "prompt": {
                    "type": "string",
                    "minLength": 1,
                    "maxLength": 1000
                },
                "aspectRatio": {
                    "type": "string",
                    "enum": [
                        "16:9",
                        "9:16"
                    ],
                    "default": "16:9"
                },
                "personGeneration": {
                    "type": "string",
                    "enum": [
                        "dont_allow",
                        "allow_adult"
                    ],
                    "default": "dont_allow"
                },
                "numberOfVideos": {
                    "type": "number",
                    "enum": [
                        1,
                        2
                    ],
                    "default": 1
                },
                "durationSeconds": {
                    "type": "number",
                    "minimum": 5,
                    "maximum": 8,
                    "default": 5
                },
                "enhancePrompt": {
                    "type": [
                        "boolean",
                        "string"
                    ],
                    "default": false
                },
                "negativePrompt": {
                    "type": "string",
                    "default": ""
                },
                "includeFullData": {
                    "type": [
                        "boolean",
                        "string"
                    ],
                    "default": false
                },
                "autoDownload": {
                    "type": [
                        "boolean",
                        "string"
                    ],
                    "default": true
                }
            },
            "required": [
                "prompt"
            ],
            "additionalProperties": false,
            "$schema": "http://json-schema.org/draft-07/schema#"
        }
    },
