[
    {
        "name": "updated_documents",
        "description": "Search documents that have been updated after a certain date or within a date interval",
        "parameters": {
            "properties": {
                "sd": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "format": "date-time",
                            "type": "string"
                        }
                    ],
                    "description": "External Docs",
                    "title": "Sd"
                },
                "ed": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "format": "date-time",
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "External Docs",
                    "title": "Ed"
                },
                "c": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "See the Supported countries section in the Developers Manual ",
                    "title": "C"
                },
                "l": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "See the Supported countries section in the Developers Manual. ",
                    "title": "L"
                },
                "t": {
                    "default": null,
                    "description": "Document type, one of: pil (Patient Information Leaflet), spc (Summary of Product Characteristics), par (Public Assessment Report), sds (Safety Data Sheet), man (User Manual), inf (Product Information), mmr (Additional Risk Minimisation Measures), hta (Health Technology Assessment Report)",
                    "enum": [
                        "pil",
                        "spc",
                        "par",
                        "sds",
                        "man",
                        "inf",
                        "mmr",
                        "hta",
                        null
                    ],
                    "title": "T"
                },
                "limit": {
                    "anyOf": [
                        {
                            "type": "integer"
                        },
                        {
                            "type": "number"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "Default value: 32 Max value: 100 Default: 0",
                    "title": "Limit"
                },
                "_from": {
                    "anyOf": [
                        {
                            "type": "integer"
                        },
                        {
                            "type": "number"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "Default: 0 ",
                    "title": "From"
                },
                "f": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "format": "date-time",
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "Enum Field optional parameter, if present it can only have 1 value. Perform date comparison on this attribute (date when record was updated in the myHealthbox database) instead of the default one (date when the actual document was updated).",
                    "title": "F"
                }
            },
            "required": [
                "sd"
            ],
            "type": "object"
        }
    },
