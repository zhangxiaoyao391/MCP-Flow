[
    {
        "name": "search_images",
        "description": "Get real-time image search results from across the web. Supports all Google Images search filters.",
        "parameters": {
            "properties": {
                "query": {
                    "description": "Search query / keyword",
                    "title": "Query",
                    "type": "string"
                },
                "limit": {
                    "anyOf": [
                        {
                            "type": "integer"
                        },
                        {
                            "type": "number"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "Maximum number of results to return. Default: 10 Allowed values: 1-100 Default: 10",
                    "title": "Limit"
                },
                "size": {
                    "default": null,
                    "description": "Find images of a specific size. Default: any Allowed values: any, large, medium, icon, 400x300_and_more, 640x480_and_more, 800x600_and_more, 1024x768_and_more, 2mp_and_more, 4mp_and_more, 6mp_and_more, 8mp_and_more, 10mp_and_more, 12mp_and_more, 15mp_and_more, 20mp_and_more, 40mp_and_more, 70mp_and_more",
                    "enum": [
                        "any",
                        "large",
                        "medium",
                        "icon",
                        "400x300_and_more",
                        "640x480_and_more",
                        "800x600_and_more",
                        "1024x768_and_more",
                        "2mp_and_more",
                        "4mp_and_more",
                        "6mp_and_more",
                        "8mp_and_more",
                        "10mp_and_more",
                        "12mp_and_more",
                        "15mp_and_more",
                        "20mp_and_more",
                        "40mp_and_more",
                        "70mp_and_more",
                        null
                    ],
                    "title": "Size"
                },
                "color": {
                    "default": null,
                    "description": "Find images with a specific dominant color. Default: any Allowed values: any, red, orange, yellow, green, teal, blue, purple, pink, white, gray, black, brown, full, transparent, grayscale",
                    "enum": [
                        "any",
                        "red",
                        "orange",
                        "yellow",
                        "green",
                        "teal",
                        "blue",
                        "purple",
                        "pink",
                        "white",
                        "gray",
                        "black",
                        "brown",
                        "full",
                        "transparent",
                        "grayscale",
                        null
                    ],
                    "title": "Color"
                },
                "type": {
                    "default": null,
                    "description": "Find images of a specific type. Default: any Allowed values: any, face, photo, clipart, lineart, animated",
                    "enum": [
                        "any",
                        "face",
                        "photo",
                        "clipart",
                        "lineart",
                        "animated",
                        null
                    ],
                    "title": "Type"
                },
                "time": {
                    "default": null,
                    "description": "Find images last updated in a specific time range. Default: any Allowed values: any, day, week, month, year",
                    "enum": [
                        "any",
                        "day",
                        "week",
                        "month",
                        "year",
                        null
                    ],
                    "title": "Time"
                },
                "usage_rights": {
                    "default": null,
                    "description": "Find images with specific usage rights / license / copyright. Default: any Allowed values: any, creative_commons, commercial",
                    "enum": [
                        "any",
                        "creative_commons",
                        "commercial",
                        null
                    ],
                    "title": "Usage Rights"
                },
                "file_type": {
                    "default": null,
                    "description": "Find images of a specific format / file type. Default: any Allowed values: any, jpg, jpeg, png, gif, svg, webp, ico, raw",
                    "enum": [
                        "any",
                        "jpg",
                        "jpeg",
                        "png",
                        "gif",
                        "svg",
                        "webp",
                        "ico",
                        "raw",
                        null
                    ],
                    "title": "File Type"
                },
                "aspect_ratio": {
                    "default": null,
                    "description": "Find images with a specific aspect ratio. Default: any Allowed values: any, tall, square, wide, panoramic",
                    "enum": [
                        "any",
                        "tall",
                        "square",
                        "wide",
                        "panoramic",
                        null
                    ],
                    "title": "Aspect Ratio"
                },
                "country": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "Find images published in a specific country / region. Allowed values: 2-letter country code, see ISO 3166-1 alpha-2",
                    "title": "Country"
                },
                "safe_search": {
                    "default": null,
                    "description": "How to show explicit content in your search results, like sexual activity and graphic violence. Default: blur Allowed values: off, blur, on",
                    "enum": [
                        "off",
                        "blur",
                        "on",
                        null
                    ],
                    "title": "Safe Search"
                },
                "region": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "The country / region from which to make the query. Default: us Allowed values: 2-letter country code, see ISO 3166-1 alpha-2",
                    "title": "Region"
                },
                "fields": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "A comma separated list of image fields to include in the response (field projection). By default all fields are returned. Example: title,size,thumbnail_url,position",
                    "title": "Fields"
                }
            },
            "required": [
                "query"
            ],
            "type": "object"
        }
    }
]