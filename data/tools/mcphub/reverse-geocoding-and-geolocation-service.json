    {
        "name": "get_nearest_cities",
        "description": "Returns a readable place name as nearest 3 cities with population, country and distance based on given latitude/longitude pair.",
        "parameters": {
            "properties": {
                "latitude": {
                    "anyOf": [
                        {
                            "type": "integer"
                        },
                        {
                            "type": "number"
                        }
                    ],
                    "description": "latitude in decimal degrees (wgs84) Default: 53.55196",
                    "title": "Latitude"
                },
                "longitude": {
                    "anyOf": [
                        {
                            "type": "integer"
                        },
                        {
                            "type": "number"
                        }
                    ],
                    "description": "longitude in decimal degrees (wgs84) Default: 9.98558",
                    "title": "Longitude"
                },
                "range": {
                    "anyOf": [
                        {
                            "type": "integer"
                        },
                        {
                            "type": "number"
                        }
                    ],
                    "description": "max radial range for lookup in meter (0=no range) Default: 0",
                    "title": "Range"
                }
            },
            "required": [
                "latitude",
                "longitude",
                "range"
            ],
            "type": "object"
        }
    },
