[
    {
        "name": "recipes_auto_complete",
        "description": "Get auto complete suggestions by name or ingredients",
        "parameters": {
            "properties": {
                "prefix": {
                    "description": "Food name or ingredient",
                    "title": "Prefix",
                    "type": "string"
                }
            },
            "required": [
                "prefix"
            ],
            "type": "object"
        }
