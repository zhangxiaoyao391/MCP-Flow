    {
        "name": "recipes_list",
        "description": "List recipes by option filters or name",
        "parameters": {
            "properties": {
                "_from": {
                    "anyOf": [
                        {
                            "type": "integer"
                        },
                        {
                            "type": "number"
                        }
                    ],
                    "description": "The offset of items to be ignored in response for paging Default: 0",
                    "title": "From"
                },
                "size": {
                    "anyOf": [
                        {
                            "type": "integer"
                        },
                        {
                            "type": "number"
                        }
                    ],
                    "description": "Number of items returned per response Default: 20",
                    "title": "Size"
                },
                "tags": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "Get suitable values from /tags/list API",
                    "title": "Tags"
                },
                "q": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "Name of food or, ingredients to search by",
                    "title": "Q"
                },
                "sort": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "Leave empty to sort by popular as default OR one of the following : approved_at:desc|approved_at:asc",
                    "title": "Sort"
                }
            },
            "required": [
                "_from",
                "size"
            ],
            "type": "object"
        }
    },
