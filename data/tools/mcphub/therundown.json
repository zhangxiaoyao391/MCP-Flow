    {
        "name": "schedules",
        "description": "Get the schedule for a sportID. Use the `from` parameter to change the starting date in the format of `yyyy-mm-dd` (ex: `2020-09-20`), which defaults to today. Use a `limit` parameter to set the number of events returned in the response. The max is `500` and the default is `50`. The schedules are ordered by `date_event` ascending, so send the largest or last `date_event` value current response to get the next available page.",
        "parameters": {
            "properties": {
                "_from": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "format": "date-time",
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "Date (yyyy-mm-dd)",
                    "title": "From"
                },
                "limit": {
                    "anyOf": [
                        {
                            "type": "integer"
                        },
                        {
                            "type": "number"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "Max value is 500. Default is 50 Default: 100",
                    "title": "Limit"
                }
            },
            "type": "object"
        }
