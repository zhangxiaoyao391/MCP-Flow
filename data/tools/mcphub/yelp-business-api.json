[
    {
        "name": "search_yelp",
        "description": "Use the same search box on yelp.com",
        "parameters": {
            "properties": {
                "location": {
                    "description": "Enter exact locations. For example, use Roosevelt, NY not Roosevelt only.",
                    "title": "Location",
                    "type": "string"
                },
                "search_term": {
                    "description": "Enter any search term you want, just like on Yelp. Ex. Coffee shop, Pizza shop, electrician, or plumber Ex. Black Owned Saloon, Mexican pizza shop",
                    "title": "Search Term",
                    "type": "string"
                },
                "limit": {
                    "anyOf": [
                        {
                            "type": "integer"
                        },
                        {
                            "type": "number"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "Number of results per page. Max: 40 Default: 10. Default: 10",
                    "title": "Limit"
                },
                "offset": {
                    "anyOf": [
                        {
                            "type": "integer"
                        },
                        {
                            "type": "number"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "If offset is set to 0, it means start from zero. If offset is set to 20, it means to start showing after 20 results. Default: 0",
                    "title": "Offset"
                },
                "business_details_type": {
                    "default": null,
                    "description": "Basic: provides basic info's about the businesses. Advanced: provides in-depth information about the businesses (it's like using /search and /each business details endpoints at the same time) Advanced option costs 2 requests per call.",
                    "enum": [
                        "basic",
                        "advanced",
                        null
                    ],
                    "title": "Business Details Type"
                }
            },
            "required": [
                "location",
                "search_term"
            ],
            "type": "object"
        }
    },
    {
        "name": "business_details",
        "description": "Scrape By Yelp URL: Ex. https://www.yelp.com/biz/capital-blossom-day-spa-washington or by business ids found from /search endpoint. You can get these business urls from the \"/search\" endpoint('YelpURL')",
        "parameters": {
            "properties": {
                "business_url": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "Get the business details by Yelp Business URL.",
                    "title": "Business Url"
                },
                "business_ids": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "Get business details from business_id found from /search endpoint. Separate each using a comma. You can put up to 39 business ids on each request. Ex. BCUhfgjbVVvjs0ro4ATRsg,wj7ekipyvssV3Ok7p8zxGg, V2_qfjnwAVWqIphf7y866w",
                    "title": "Business Ids"
                }
            },
            "type": "object"
        }
    },
