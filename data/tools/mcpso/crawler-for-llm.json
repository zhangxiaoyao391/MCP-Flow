[
    {
        "name": "langgraph-crawler_crawl_tool",
        "description": "A powerful web content extraction tool that retrieves and processes raw content from specified URLs, ideal for data collection, content analysis, and research tasks.",
        "parameters": {
            "type": "object",
            "properties": {
                "url": {
                    "type": "string",
                    "format": "uri",
                    "description": "the url to crawl"
                },
                "raw": {
                    "type": "boolean",
                    "default": false,
                    "description": "return raw html"
                }
            },
            "required": [
                "url"
            ],
            "additionalProperties": false,
            "$schema": "http://json-schema.org/draft-07/schema#"
        }
    },
    {
        "name": "langgraph-crawler_web_search_tool",
        "description": "A powerful web search tool that provides comprehensive, real-time results using search engine. Returns relevant web content with customizable parameters for result count, content type, and domain filtering. Ideal for gathering current information, news, and detailed web content analysis.",
        "parameters": {
            "type": "object",
            "properties": {
                "query": {
                    "type": "string",
                    "description": "the query to search"
                },
                "engines": {
                    "type": "array",
                    "items": {
                        "type": "string",
                        "enum": [
                            "basic",
                            "npm",
                            "juejin",
                            "anthropic",
                            "github",
                            "bing"
                        ]
                    },
                    "default": [
                        "bing"
                    ],
                    "description": "the engines to use"
                },
                "returnType": {
                    "type": "string",
                    "enum": [
                        "json",
                        "markdown"
                    ],
                    "default": "json",
                    "description": "the content type to return"
                },
                "withMetadata": {
                    "type": "boolean",
                    "default": true,
                    "description": "whether to include metadata in the search results"
                }
            },
            "required": [
                "query"
            ],
            "additionalProperties": false,
            "$schema": "http://json-schema.org/draft-07/schema#"
        }
    }
]