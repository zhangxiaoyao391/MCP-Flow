[
    {
        "name": "dice_parse_dice_roll_notation",
        "description": "Parses dice roll notation",
        "parameters": {
            "type": "object",
            "properties": {
                "notation": {
                    "type": "string",
                    "minLength": 1,
                    "description": "The dice roll notation to parse"
                }
            },
            "required": [
                "notation"
            ],
            "additionalProperties": false,
            "$schema": "http://json-schema.org/draft-07/schema#"
        }
    },
    {
        "name": "dice_dice_roll",
        "description": "Make a dice roll",
        "parameters": {
            "type": "object",
            "properties": {
                "type": {
                    "type": "string",
                    "enum": [
                        "standard",
                        "fate"
                    ]
                },
                "count": {
                    "type": "integer",
                    "minimum": 0
                },
                "sides": {
                    "type": "integer",
                    "exclusiveMinimum": 0
                },
                "modifier": {
                    "type": "integer"
                }
            },
            "required": [
                "type",
                "count",
                "sides"
            ],
            "additionalProperties": false,
            "$schema": "http://json-schema.org/draft-07/schema#"
        }
    }
]