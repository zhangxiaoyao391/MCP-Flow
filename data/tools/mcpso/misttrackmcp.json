        "description": "Recursively analyze transaction relationships and build transaction graph",
        "parameters": {
            "type": "object",
            "properties": {
                "coin": {
                    "type": "string",
                    "description": "Cryptocurrency type, such as ETH, BTC, etc."
                },
                "address": {
                    "type": "string",
                    "description": "Address to analyze"
                },
                "max_depth": {
                    "type": "number",
