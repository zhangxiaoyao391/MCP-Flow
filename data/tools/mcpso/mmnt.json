[
    {
        "name": "mmnt_mmnt_search",
        "description": "Search in Mamont search engine",
        "parameters": {
            "type": "object",
            "properties": {
                "query": {
                    "type": "string",
                    "description": "query string"
                },
                "page": {
                    "type": "number",
                    "description": "page number"
                }
            },
            "required": [
                "query",
                "page"
            ],
            "additionalProperties": false,
            "$schema": "http://json-schema.org/draft-07/schema#"
        }
    },
    {
        "name": "mmnt_mmnt_cache",
        "description": "Extract page from Mamont cache",
        "parameters": {
            "type": "object",
            "properties": {
                "id": {
                    "type": "string",
                    "description": "unique cache id"
                },
                "onlyText": {
                    "type": "boolean",
                    "description": "Should the result be text only (no html)"
                }
            },
            "required": [
                "id",
                "onlyText"
            ],
            "additionalProperties": false,
            "$schema": "http://json-schema.org/draft-07/schema#"
        }
    }
]