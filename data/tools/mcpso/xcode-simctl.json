[
    {
        "name": "simctl_simctl_list_devices",
        "description": "\nList available iOS simulators and their states.\n\nArgs:\n    format: Output format (json or text). Defaults to json.\n    filter: Optional filter term (e.g., 'available', 'iPhone', 'iOS 17')\n    \nReturns:\n    List of available simulators\n",
        "parameters": {
            "properties": {
                "format": {
                    "default": "json",
                    "title": "Format",
                    "type": "string"
                },
                "filter": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "title": "Filter"
                }
            },
            "title": "simctl_list_devicesArguments",
            "type": "object"
        }
    },
    {
        "name": "simctl_simctl_boot_device",
