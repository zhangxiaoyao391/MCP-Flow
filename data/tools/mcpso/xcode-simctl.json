                    "type": "string"
                },
                "bundle_id": {
                    "title": "Bundle Id",
                    "type": "string"
                },
                "wait_for_debugger": {
                    "default": false,
                    "title": "Wait For Debugger",
                    "type": "boolean"
                },
                "console_mode": {
                    "default": "none",
                    "title": "Console Mode",
                    "type": "string"
