[
    {
        "name": "career-mcp_search_jobs_tool",
        "description": "\n    Search for jobs using Careerjet API.\n\n    Args:\n        keywords: Keywords to match job titles, content or company names\n        location: Location of requested jobs (e.g., \"London\", \"New York\", \"Berlin\")\n        locale: Locale code (default: en_US). Examples: en_GB, en_US, de_DE, fr_FR\n        affid: Affiliate ID provided by Careerjet (required)\n        user_ip: IP address of the end-user\n        user_agent: User agent of the end-user's browser\n        url: URL of page that will display the search results\n        sort: Sort type - 'relevance' (default), 'date', or 'salary'\n        start_num: Position of returned job postings (>= 1)\n        pagesize: Number of jobs returned in one call\n        page: Page number of returned jobs (>= 1)\n        contracttype: Contract type - 'p' (permanent), 'c' (contract), 't' (temporary), 'i' (training), 'v' (voluntary)\n        contractperiod: Contract period - 'f' (full time), 'p' (part time)\n\n    Returns:\n        dict: Search results from Careerjet API including job listings\n    ",
        "parameters": {
            "properties": {
                "keywords": {
                    "title": "Keywords",
                    "type": "string"
                },
                "location": {
                    "title": "Location",
                    "type": "string"
                },
                "locale": {
                    "default": "en_US",
                    "title": "Locale",
                    "type": "string"
                },
                "affid": {
                    "default": "213e213hd12344552",
                    "title": "Affid",
                    "type": "string"
                },
                "user_ip": {
                    "default": "127.0.0.1",
                    "title": "User Ip",
                    "type": "string"
                },
                "user_agent": {
                    "default": "Mozilla/5.0 (compatible; MCP-CareerjetBot/1.0)",
                    "title": "User Agent",
                    "type": "string"
                },
                "url": {
                    "default": "http://example.com/jobsearch",
                    "title": "Url",
                    "type": "string"
                },
                "sort": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "title": "Sort"
                },
                "start_num": {
                    "anyOf": [
                        {
                            "type": "integer"
                        },
                        {
                            "type": "null"
                        }
