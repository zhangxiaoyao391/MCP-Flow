[
    {
        "name": "open-websearch_search",
        "description": "Search the web using multiple engines (e.g., Baidu, Bing, DuckDuckGo, CSDN, Exa, Brave, Juejin(\u6398\u91d1)) with no API key required",
        "parameters": {
            "type": "object",
            "properties": {
                "query": {
                    "type": "string",
                    "minLength": 1
                },
                "limit": {
                    "type": "number",
                    "minimum": 1,
                    "maximum": 50,
                    "default": 10
                },
                "engines": {
                    "type": "array",
                    "items": {
                        "type": "string",
                        "enum": [
                            "baidu",
                            "bing",
                            "csdn",
                            "duckduckgo",
                            "exa",
                            "brave",
                            "juejin"
                        ]
                    },
                    "minItems": 1,
                    "default": [
                        "bing"
                    ]
                }
            },
            "required": [
                "query"
            ],
            "additionalProperties": false,
            "$schema": "http://json-schema.org/draft-07/schema#"
        }
    },
    {
        "name": "open-websearch_fetchLinuxDoArticle",
        "description": "Fetch full article content from a linux.do post URL",
        "parameters": {
            "type": "object",
            "properties": {
                "url": {
                    "type": "string",
                    "format": "uri"
                }
            },
            "required": [
                "url"
            ],
            "additionalProperties": false,
            "$schema": "http://json-schema.org/draft-07/schema#"
        }
    },
    {
        "name": "open-websearch_fetchCsdnArticle",
        "description": "Fetch full article content from a csdn post URL",
        "parameters": {
            "type": "object",
            "properties": {
                "url": {
                    "type": "string",
                    "format": "uri"
                }
            },
            "required": [
                "url"
            ],
            "additionalProperties": false,
            "$schema": "http://json-schema.org/draft-07/schema#"
        }
    },
    {
        "name": "open-websearch_fetchGithubReadme",
        "description": "Fetch README content from a GitHub repository URL",
        "parameters": {
            "type": "object",
            "properties": {
                "url": {
                    "type": "string",
                    "minLength": 1
                }
            },
            "required": [
                "url"
            ],
            "additionalProperties": false,
            "$schema": "http://json-schema.org/draft-07/schema#"
        }
    },
    {
        "name": "open-websearch_fetchJuejinArticle",
        "description": "Fetch full article content from a Juejin(\u6398\u91d1) post URL",
        "parameters": {
            "type": "object",
            "properties": {
                "url": {
                    "type": "string",
                    "format": "uri"
                }
            },
            "required": [
                "url"
            ],
            "additionalProperties": false,
            "$schema": "http://json-schema.org/draft-07/schema#"
        }
    }
]