            "type": "object",
            "properties": {
                "url": {
                    "type": "string",
                    "description": "Swagger/OpenAPI document URL"
                },
                "headers": {
                    "type": "object",
                    "additionalProperties": {
                        "type": "string"
                    },
                    "description": "Request headers"
                },
                "includeSchemas": {
                    "type": "boolean",
                    "description": "Whether to include schema definitions"
                },
                "includeDetails": {
