    {
        "name": "tusclasesparticulares-mcp_tusclasesparticulares_bulk_operations",
        "description": "Perform bulk operations on multiple items",
        "parameters": {
            "type": "object",
            "properties": {
                "operation": {
                    "type": "string",
                    "enum": [
                        "bulk_message",
                        "bulk_buy_leads",
                        "bulk_update_publications"
                    ],
                    "description": "Type of bulk operation to perform"
                },
                "message": {
                    "type": "string",
                    "description": "Message template for bulk messaging"
                },
                "count": {
                    "type": "number",
                    "description": "Number of items to process",
                    "default": 5
                },
                "delayBetween": {
                    "type": "number",
                    "description": "Delay between operations in seconds",
                    "default": 2
                }
            },
            "required": [
                "operation"
            ]
        }
