[
    {
        "name": "velt-analytics-mcp_get_events_analytics",
        "description": "Get paginated events from Velt with comprehensive filtering and timezone support. \n        Supports three filtering modes:\n        1. Date range: Use startDate and endDate for a specific period\n        2. Single date: Use date for events from a specific day\n        3. Fallback: Use lastDaysCount for recent events (default: 30 days)\n        \n        Returns events with event_name, event_timestamp (ISO format), properties, and unique id.\n        Supports pagination with nextPageToken for retrieving subsequent pages.",
        "parameters": {
            "type": "object",
            "properties": {
                "data": {
                    "type": "object",
                    "properties": {
                        "pageSize": {
                            "type": "integer",
                            "minimum": 1,
                            "maximum": 100,
                            "default": 50,
                            "description": "Number of events to return per page"
                        },
                        "pageToken": {
                            "type": "string",
                            "default": "",
                            "description": "Token for pagination to get next page of results"
                        },
                        "filter": {
                            "type": "object",
                            "properties": {
                                "lastDaysCount": {
                                    "type": "integer",
                                    "minimum": 1,
                                    "maximum": 365,
                                    "default": 7,
                                    "description": "Number of days to look back (fallback when no specific date is provided)"
                                },
                                "eventName": {
                                    "type": "string",
                                    "default": "",
                                    "description": "Filter by specific event name"
                                },
                                "date": {
                                    "type": "string",
                                    "description": "Filter by specific date (ISO format)"
                                },
                                "userId": {
                                    "type": "string",
                                    "description": "Filter by specific user ID"
                                },
                                "startDate": {
                                    "type": "string",
                                    "description": "Start date for date range filtering (ISO format)"
                                },
                                "endDate": {
                                    "type": "string",
                                    "description": "End date for date range filtering (ISO format)"
                                }
                            },
                            "additionalProperties": false,
                            "default": {},
                            "description": "Filter criteria for events - supports single date, date range (startDate/endDate), or fallback to lastDaysCount"
                        },
                        "timezone": {
                            "type": "string",
                            "default": "America/Los_Angeles",
                            "description": "Timezone for date filtering and timestamp conversion"
                        }
                    },
                    "additionalProperties": false
                }
            },
            "required": [
                "data"
            ],
            "additionalProperties": false,
            "$schema": "http://json-schema.org/draft-07/schema#"
        }
    },
