{
  "mcpServers": {
    "biggo-mcp-server": {
      "command": "uvx",
      "args": [
        "BigGo-MCP-Server@latest"
      ],
      "env": {
        "BIGGO_MCP_SERVER_CLIENT_ID": "CLIENT_ID",
        "BIGGO_MCP_SERVER_CLIENT_SECRET": "CLIENT_SECRET",
        "BIGGO_MCP_SERVER_REGION": "REGION"
      }
    }
  }
}