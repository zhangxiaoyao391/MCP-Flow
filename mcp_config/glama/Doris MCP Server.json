{
  "mcpServers": {
    "doris-stdio": {
      "command": "doris-mcp-server",
      "args": [
        "--transport",
        "stdio"
      ],
      "env": {
        "DORIS_HOST": "127.0.0.1",
        "DORIS_PORT": "9030",
        "DORIS_USER": "root",
        "DORIS_PASSWORD": "your_db_password"
      }
    }
  }
}